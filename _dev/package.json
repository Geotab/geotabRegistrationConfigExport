{
  "name": "RegistrationConfig",
  "version": "0.0.1",
  "author": "nickklimkovich@geotab.com",
  "description": "Tool for exporting default registration config in JSON file",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "systemjs": "^0.19.31",
    "startServer": "node ../../../start.js",
    "watch": "watchify sources/addin.ts -p tsify --debug -o ../public/addin.js -dv",
    "build": "browserify sources/addin.ts -p tsify > ../public/addin.js",
    "start": "start npm run startServer & start npm run watch",
    "copyfiles": "copyfiles -u 2 ../public/* ../public/*/*/* ../../../MyGeotabDevTool/addins/DatabaseExportConfig/"
  },
  "dependencies": {
    "browserify": "^16.2.3",
    "copyfiles": "^2.2.0",
    "tsify": "^4.0.1",
    "typescript": "^3.5.2",
    "watchify": "^3.11.1"
  }
}
